"call plug#begin('~/.vim/plugged')
"call plug#end()
set nocompatible
filetype indent plugin on
syntax on
set number
set autoindent
set smartindent
set wrap
set mouse=a
set showmatch
set cursorline
set tabstop=4
set shiftwidth=4
set ruler
"search
set hlsearch
set incsearch
set ignorecase
set smartcase
